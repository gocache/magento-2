<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Backend\Block\Cache\Additional" name="cache.purge.url" template="GoCache_CDN::system/cache/purgeUrl.phtml">
                <arguments>
                    <argument name="permissions" xsi:type="object">Magento\Backend\Block\Cache\Permissions</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
